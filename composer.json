{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "altek/accountant": "^3.0",
        "altek/eventually": "^2.0",
        "awobaz/compoships": "^2.0",
        "binarytorch/larecipe": "^2.4",
        "bnbwebexpertise/laravel-attachments": "^1.0",
        "cloudstudio/resource-generator": "^1.5",
        "ek0519/quilljs": "^0.3.1",
        "eminiarts/nova-tabs": "^1.3",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "inertiajs/inertia-laravel": "^0.4.3",
        "jenssegers/mongodb": "^3.8",
        "kabbouchi/nova-impersonate": "^1.7",
        "laravel/framework": "^8.12",
        "laravel/jetstream": "^2.0",
        "laravel/nova": "~3.0",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "^3.1",
        "matthewbdaly/laravel-azure-storage": "^1.6",
        "metrogistics/laravel-azure-ad-oauth": "^1.2",
        "nunomaduro/laravel-console-menu": "^3.2",
        "optimistdigital/nova-settings": "^3.1",
        "optimistdigital/nova-sortable": "^2.2",
        "owen-it/laravel-auditing": "^12.0",
        "pusher/pusher-php-server": "^5.0",
        "sentry/sentry-laravel": "^2.4",
        "silvanite/brandenburg": "^1.4",
        "silvanite/novatoolpermissions": "^1.0",
        "socialiteproviders/microsoft-azure": "^4.0",
        "softcreatr/jsonpath": "^0.5 || ^0.7",
        "spatie/laravel-model-states": "^2.1",
        "staudenmeir/eloquent-has-many-deep": "^1.7",
        "thomasjohnkane/snooze": "^1.0",
        "tightenco/ziggy": "^1.0",
        "wehaa/custom-links": "^0.1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.9",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laracademy/generators": "^3.0",
        "laravel/envoy": "^2.7",
        "laravel/sail": "^0.0.5",
        "laravel/telescope": "^4.4",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
