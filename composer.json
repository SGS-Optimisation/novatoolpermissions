{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "awobaz/compoships": "^2.0",
        "barryvdh/laravel-ide-helper": "^2.9",
        "eminiarts/nova-tabs": "^1.3",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "inertiajs/inertia-laravel": "^0.2.4",
        "jenssegers/mongodb": "^3.8",
        "laravel/framework": "^8.12",
        "laravel/jetstream": "^1.6",
        "laravel/nova": "~3.0",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.5",
        "metrogistics/laravel-azure-ad-oauth": "^1.2",
        "optimistdigital/nova-settings": "^3.1",
        "optimistdigital/nova-sortable": "^1.6",
        "silvanite/brandenburg": "^1.4",
        "silvanite/novatoolpermissions": "^1.0",
        "socialiteproviders/microsoft-azure": "^3.0",
        "tightenco/ziggy": "^0.9.4",
        "ext-json": "*"
    },
    "require-dev": {
        "cloudstudio/resource-generator": "9999999-dev",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laracademy/generators": "^3.0",
        "laravel-shift/blueprint": "9999999-dev",
        "laravel/sail": "^0.0.5",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
